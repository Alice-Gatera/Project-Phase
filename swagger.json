{
  "openapi": "3.0.0",
  "info": {
    "title": "Barefoot Nomad",
    "description": "Barefoot nomad is an application that will enable its “Company Nomads” book their international travel and accommodation globally; easily and conveniently across all the locations/centers where the Company has its operations.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "HTTPS",
      "url": "HTTP"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "description": "Welcome page of Barefoot Nomad",
        "responses": {
          "200": {
            "description": "Successful",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Welcome to Barefoot Nomad"
                }
              }
            }
          }
        }
      }
    },
     "/api/v1/login":{
      "post":{
        "description":"User login with email and password",
        "parameter":{
          "name":"User login",
          "in":"boddy",
          "required":true,
          "description":"User login will be json object",
          "schema":{
            "type":"object",
            "properties":{
              "email":{
                "type":"string",
                "example":"habajeunes@gmail.com"
              },
              "password":{
                "type":"string",
                "example":"@12345ami!"
              }
            }
          }

        },
        "responses":{
          "200":{
            "application/json":{
              "description":"message:login successful! and genarate token",
              "schema":{
                "type":"string",
                "example":"login successful!"
              }
              
            }
          }
        }
      }
    }
  }
}