{
  "name": "c2-barefoot-backend",
  "version": "1.0.0",
  "description": "Backend API for Barefoot Nomad ",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=production && nodemon  --exec babel-node app.js",
    "devStart":"set NODE_ENV=development && nodemon  --exec babel-node app.js",
    "createModel":"cd src && npx sequelize-cli model:generate ",
    "migrate":"cd src && npx sequelize-cli db:migrate",
    "migrate-undo":"cd src && npx sequelize-cli db:migrate:undo",
    "createSeed":"cd src && npx sequelize-cli seed:generate",
    "seed":"cd src && npx sequelize-cli db:seed:all",
    "seed-undo":"cd src && npx sequelize-cli db:seed:undo",
    "seed-undo-all":"cd src && npx sequelize-cli db:seed:undo:all",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/c2-barefoot-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/c2-barefoot-backend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/c2-barefoot-backend#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.4.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4"
  }
}
